# Statistical Model Configuration
# Copy this file to .env and modify as needed

# Algorithm Selection
R_CONTOUR_ALGORITHM=Active_Contour
R_MODEL_FEATURE=NEW_X_NS_XY

# Paths
R_ROOT_PATH=../

# Shoe Parameters
R_COL_SHOE=307
R_ROW_SHOE=395
R_NUM_SHOE=387

# Relevant Area Parameters
R_REL_COL_SHOE=150
R_REL_ROW_SHOE=300
R_REL_X_CORD=0.25
R_REL_Y_CORD=0.5

# Processing Options
R_PARALLEL_PROCESSING=true
R_VERBOSE_OUTPUT=true
R_SAVE_INTERMEDIATE=true

# For different datasets, uncomment and modify:
# R_CONTOUR_ALGORITHM=Convex
# R_MODEL_FEATURE=NS_XY
# R_NUM_SHOE=200